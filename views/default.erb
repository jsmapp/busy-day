<!DOCTYPE HTML>
<html>
	<head>
		<title>{{ page.title | default(site.title) }}</title>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<meta name="generator" content="Spress v{{ spress.version }}">
		<meta name="description" content="{{ page.description | default(site.description) }}" />
		<meta name="keywords" content="{{ page.keywords | default(site.keywords) | join(',') }}" />
		<!--[if lte IE 8]><script src="/assets/css/ie/html5shiv.js"></script><![endif]-->
		<script src="js/jquery/dist/jquery.min.js"></script>
		<script src="js/skel/dist/skel.min.js"></script>
		<script src="js/init.js"></script>
		<noscript>
			<link rel="stylesheet" href="css/skel.css" />
			<link rel="stylesheet" href="css/style.css" />
			<link rel="stylesheet" href="css/style-wide.css" />
		</noscript>

		<!--[if lte IE 8]><link rel="stylesheet" href="{{ site.url }}/assets/css/ie/v8.css" /><![endif]-->
		<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/styles/default.min.css">
		<link rel="stylesheet" href="css/highlight-theme.css">

	</head>
	<body>
		<div id="menu-top">
			{% include 'menu.html' with { menu: site.menu.top } %}
		</div>

		{% block content %}
            {{ page.content }}
        {% endblock %}

        <div id="footer">
			<div class="container 75%">

				<ul class="icons">
					{% if site.social_networks.twitter %}
					<li>
						<a href="https://twitter.com/{{ site.social_networks.twitter }}" class="icon fa-twitter">
							<span class="label">Twitter</span>
						</a>
					</li>
					{% endif %}
					{% if site.social_networks.facebook %}
					<li>
						<a href="{{ site.social_networks.facebook  }}" class="icon fa-facebook">
							<span class="label">Facebook</span>
						</a>
					</li>
					{% endif %}
					{% if site.social_networks.instagram %}
					<li>
						<a href="http://instagram.com/{{ site.social_networks.instagram }}" class="icon fa-instagram">
							<span class="label">Instagram</span>
						</a>
					</li>
					{% endif %}
					{% if site.social_networks.github %}
					<li>
						<a href="{{ site.social_networks.github }}" class="icon fa-github">
							<span class="label">Github</span>
						</a>
					</li>
					{% endif %}
					{% if site.social_networks.dribbble %}
					<li>
						<a href="https://dribbble.com/{{ site.social_networks.dribbble }}" class="icon fa-dribbble">
							<span class="label">Dribbble</span>
						</a>
					</li>
					{% endif %}
				</ul>

				<div id="menu-bottom">
					{% include 'menu.html' with { menu: site.menu.bottom } %}
				</div>

				<ul class="copyright">
					<li>
						&copy; {{ 'now' | date('Y') }} {{ site.author.name }}. All rights reserved.
					</li>
					<li>
						Design: <a href="http://html5up.net">HTML5 UP</a>
					</li>
					<li>
						Powered by: <a href="http://spress.yosymfony.com">Spress</a>
					</li>
				</ul>

			</div>
		</div>

		{% block javascript %}{% endblock %}

		<script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js"></script>
		<script>hljs.initHighlightingOnLoad();</script>
	</body>
</html>